[sub_resource type="GDScript" id="GDScript_v6mhp"]
resource_name = "time"
script/source = "extends Label

@onready var start_time = Time.get_ticks_msec() / 1000
@onready var current_time = Time.get_ticks_msec() / 1000
@onready var paused_time = 0
@onready var resumed_time = 0
@onready var timer = 0

func resume_time():
	resumed_time = Time.get_ticks_msec() / 1000
	start_time = resumed_time - current_time
	paused_time = 0
	
func pause_time():
	paused_time = current_time

func _process(delta):
	if Game.gameover == false and paused_time == 0:
		current_time = Time.get_ticks_msec() / 1000
		var timer = current_time - start_time
		Game.minute = roundi(timer / 60)
		Game.seconds = timer - Game.minute * 60
		if Input.is_action_just_pressed(\"pause\"):
			print(timer)
			pause_time()
	else:
		if Input.is_action_just_released(\"pause\"):
			print(timer)
			resume_time()
	
	if Game.seconds < 10:
		text = str(Game.minute) + \":0\" + str(Game.seconds)
	else:
		text = str(Game.minute) + \":\" + str(Game.seconds)
"

